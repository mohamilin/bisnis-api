				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var headerValue = pm.response.json()?.ResponseDesc?.token?.access?.token ?? null\r",
									"var tokenRefresh = pm.response.json()?.ResponseDesc?.token?.refresh?.token ?? null\r",
									"pm.globals.set(\"token\", headerValue);\r",
									"pm.globals.set(\"refreshToken\", tokenRefresh);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
									"key": "token",
									"value": "{{token}}",
									"type": "string"
								}
							]
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
